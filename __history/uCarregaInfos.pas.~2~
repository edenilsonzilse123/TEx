unit uCarregaInfos;

interface

  procedure SetarCep(pCep:String);
  procedure SetarLogradouro(pLograd:String);

var
 vCEP, vLogradouro:String;

implementation

procedure SetarCep(pCep:String);
begin
  vCEP  :=  pCep;
end;

procedure SetarLogradouro(pLograd:String);
begin
  vLogradouro :=  pLograd;
end;

end.
