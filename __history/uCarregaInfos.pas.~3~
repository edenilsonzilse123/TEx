unit uCarregaInfos;

interface

  procedure SetarCep(pCep:String);
  procedure SetarLogradouro(pLograd:String);
  procedure SetarCidade(pCidade:String);
  procedure SetarUF(pUF:String);

var
 vCEP, vLogradouro, vCidade, vUF:String;

implementation

procedure SetarCep(pCep:String);
begin
  vCEP  :=  pCep;
end;

procedure SetarLogradouro(pLograd:String);
begin
  vLogradouro :=  pLograd;
end;

procedure SetarCidade(pCidade:String);
begin
  vCidade :=  pCidade;
end;

procedure SetarUF(pUF:String);
begin
  vUF :=  pUF;
end;

end.
